{
  "expo": {
    "name": "BabyNest",
    "slug": "BrestfeedingTracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "breastfeedingtracker",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF0F5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.breastfeedingtracker.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFF0F5"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.breastfeedingtracker.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "sounds": []
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow Breastfeeding Tracker to access your microphone for audio notes."
        }
      ]
    ]
  }
}
